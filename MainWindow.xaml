<Window x:Class="AES.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AES"
        mc:Ignorable="d"
        Title="AES" Height="256.862" Width="256.373">
    <Grid Margin="0,0,2,7">
        <Button x:Name="encryptButton" Content="Encrypt" HorizontalAlignment="Left" Height="33" Margin="9,177,0,0" VerticalAlignment="Top" Width="80" Click="encryptButton_Click"/>
        <Button x:Name="decryptButton" Content="Decrypt" HorizontalAlignment="Left" Height="33" Margin="94,177,0,0" VerticalAlignment="Top" Width="78" Click="decryptButton_Click"/>
        <TextBox x:Name="putTextBox" HorizontalAlignment="Left" Height="63" Margin="9,10,0,0" TextWrapping="Wrap" Text="Put Text Here" VerticalAlignment="Top" Width="218" GotFocus="TextBox_GotFocus" GotMouseCapture="TextBox_GotMouseCapture" IsKeyboardFocusWithinChanged="TextBox_IsMouseCaptureWithinChanged"/>
        <TextBox x:Name="keyBox" HorizontalAlignment="Left" Height="24" Margin="10,78,0,0" TextWrapping="Wrap" Text="Key here" VerticalAlignment="Top" Width="153" GotFocus="KeyBox_GotFocus" GotMouseCapture="KeyBox_GotMouseCapture"/>

    </Grid>
</Window>
